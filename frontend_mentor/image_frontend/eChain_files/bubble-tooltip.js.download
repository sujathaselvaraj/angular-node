function showToolTip(f,g){if(document.all){f=event}var d=document.getElementById("bubble_tooltip");var c=document.getElementById("bubble_tooltip_content");c.innerHTML=g;d.style.display="block";var a=Math.max(document.body.scrollTop,document.documentElement.scrollTop);if(navigator.userAgent.toLowerCase().indexOf("safari")>=0){a=0}var b=f.clientX-100;if(b<0){b=0}d.style.left=b+"px";d.style.top=f.clientY-d.offsetHeight-1+a+"px"}function hideToolTip(){document.getElementById("bubble_tooltip").style.display="none"};